{
  "name": "request-progress-ex",
  "version": "4.0.1",
  "description": "Tracks the download progress of a request made with mikeal/request, giving insight of various metrics including progress percent, download speed and time remaining",
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "@types/request": "^2.48.4",
    "request": "^2.88.0",
    "throttleit": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "expect.js": "^0.3.1",
    "nyc": "^15.0.0",
    "mocha": "^6.2.2"
  },
  "scripts": {
    "test": "mocha --bail",
    "test-cov": "nyc cover --dir test/coverage _mocha -- --bail && echo open test/coverage/lcov-report/index.html",
    "test-travis": "nyc cover _mocha --report lcovonly -- --bail && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linqingwudiv1/request-progress-ex"
  },
  "bugs": {
    "url": "http://github.com/linqingwudiv1/request-progress-ex/issues"
  },
  "keywords": [
    "progress",
    "request",
    "mikeal",
    "size",
    "bytes",
    "percent",
    "percentage",
    "speed",
    "eta",
    "etr"
  ],
  "author": "qqq",
  "license": "MIT"
}
